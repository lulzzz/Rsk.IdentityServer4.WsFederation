@{
    ViewData["Title"] = "Home Page";
}
@if (User.Identity.IsAuthenticated)
{
    <h1>Authenticated!</h1>
    <h2>Type: @User.Identity.AuthenticationType</h2>
    <ul>
        @foreach (var claim in User.Claims)
        {
            <li><strong>@claim.Type</strong>: @claim.Value</li>
        }
    </ul>
}
else
{
    <h1>Unauthenticated...</h1>
}